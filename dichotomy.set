; Численное нахождение корня уравнения методом дихотомии (бисекции)
; Y = X*X - X0
; 
SET 1, A ; левая граница начального диапазона поиска корня
SET 5, B : правая
SET 3, X0 ; константа
;
CPY A, X[1] ; создание переменных с инициализацией
CPY B, X[2]
;
for[i]=3,10,1 { ; значения по индексам 1 и 2 уже присвоены 
SET 0, X[i]  ; точка вычисления функции
}
;
for[j]=1,10,1 {  
MUL X[j],X[j],XX[j] 
SUB XX[j],X0,Y[j] ; вычисленное значение функции
;






