; пример использования мАкроса работы с псевдо-массивами
; ПРОГРАММА - заполнение псевдо-массива константой
;
; вариант 1 (использование SET)
;
for[i]=1,10,1 {
SET 1, mass_1[i] ; присваиваем всем m[...] значение 1.0
} 
;
; вариант 2 (использование CPY)
;
SET 2, work1
;
for[j]=1,10,1 {
CPY work1, mass_2[j] ; присваиваем всем m[...] значение work
} 







